{
  "Sort": ["linter", "severity", "path", "line"],
  "WarnUnmatchedDirective": true,
  "DisableAll": true,
  "Enable": [
    "deadcode",
    "errcheck",
    "gocyclo",
    "gofmt",
    "goimports",
    "golint",
    "interfacer",
    "lll",
    "maligned",
    "megacheck",
    "misspell",
    "nakedret",
    "structcheck",
    "unconvert",
    "unparam",
    "varcheck",
    "vet"
  ],
  "Cyclo": 12,
  "LineLength": 100
}
